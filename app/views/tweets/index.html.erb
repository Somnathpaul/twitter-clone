<p id="notice"><%= notice %></p>

<section class="section">
  <div class="container">
  <div class="columns">

    <%= render 'feed'%>
    <%= render 'follow_ad'%>
   </div>
  </div>
</section>

  <body>
    <% @tweets.each do |tweet| %>
      <tr>
        <td><%= tweet.tweet %></td>
        <td><%= link_to 'Show', tweet %></td>
        <td><%= link_to 'Edit', edit_tweet_path(tweet) %></td>
        <td><%= link_to 'Destroy', tweet, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </body>
</table>

