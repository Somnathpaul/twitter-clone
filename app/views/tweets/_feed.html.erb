<% @tweets.each do |t|%> 
   <div class="box">
     <article class="media">
       <div class="media-left">
          <figure class="image is-64x64">
            <img src="https://bulma.io/images/placeholders/128x128.png">
          </figure>
       </div>

       <div class="media-content">
         <div class="content">
           <strong>Somnath.paul<strong>
           <small>@somnath.geek</small>
           <p> <%= t.tweet%> </p>
         </div>
       </div>
     </div>
   </div>
<% end %>